    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "aws-cdk-lib": "2.95.1",
    "constructs": "10.2.69",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "sst": "^2.28.0"
  },
  "pnpm": {
    "overrides": {
      "@trivago/prettier-plugin-sort-imports>@babel/core": "7.20.12",
