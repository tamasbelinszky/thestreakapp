{
  "name": "thestreakapp",
  "description": "Create streaks and track your progress. How long can you go?",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:next": "pnpm set-secrets && sst bind next dev --profile self",
    "dev:sst": "sst dev --profile self",
    "build": "sst bind next build",
    "deploy:prod": "sst deploy --profile self --stage production",
    "remove:prod": "sst remove --profile self --stage production",
    "set-secret": "sst secrets set --profile self",
    "set-secrets": "sst secrets load .env --profile self",
    "start": "next start",
    "lint": "next lint",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write ."
  },
  "dependencies": {
    "@auth/dynamodb-adapter": "^1.0.2",
